{
  "name": "tradejourney-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "private",
  "engines": {
    "node": "20.18.0",
    "npm": "10.8.2"
  },
  "scripts": {
    "build": "nest build",
    "build:digitalocean": "npm install && npm run build && npm ci",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration-generate": "npm run typeorm:cli -- migration:generate ./src/migration/schema-update -d ./src/database/data-source.ts",
    "migration:show": "npm run typeorm:cli -- migration:show -d ./src/database/data-source.ts",
    "migration-run": "npm run typeorm:cli -- migration:run -d  ./src/database/data-source.ts",
    "migration:create": "npm run typeorm:cli -- migration:create ./src/migration/schema-update",
    "migration:revert": "npm run typeorm:cli -- migration:revert -d ./src/database/data-source.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@clerk/backend": "^1.3.2",
    "@clerk/clerk-sdk-node": "^5.0.16",
    "@css-inline/css-inline": "^0.14.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "^5.7.1",
    "@sendgrid/mail": "^8.1.0",
    "@types/multer": "^1.4.11",
    "@types/passport-local": "^1.0.38",
    "algoliasearch": "^4.24.0",
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "bluebird": "^3.7.2",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.3.1",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "handlebars": "^4.7.8",
    "joi": "^17.11.0",
    "jwks-rsa": "^3.1.0",
    "libphonenumber-js": "^1.11.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.13",
    "npm-check-updates": "^16.14.12",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "raw-body": "^2.5.2",
    "reflect-metadata": "^0.1.13",
